        android:left="-1dp"
        android:right="1dp"
        android:top="-1dp">
        <shape android:shape="rectangle" >

            <!-- 테두리 두께 및 색상 설정 -->
            <stroke
                android:width="1dp"
                android:color="#000000" />

            <solid android:color="#64E7AE" />
        </shape>
    </item>
</layer-list>